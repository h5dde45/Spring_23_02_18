<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.Address" table="address">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="country" type="java.lang.String">
            <column name="coyntry" length="255" not-null="true"/>
        </property>
        <property name="city" type="java.lang.String">
            <column name="city" length="255" not-null="true"/>
        </property>
        <property name="street" type="java.lang.String">
            <column name="street" length="255" not-null="true"/>
        </property>
        <property name="postCode" type="java.lang.String">
            <column name="postCode" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
